{
  "version": 2,
  "builds": [
    {
      "src": "api/generate.js",
      "use": "@vercel/node",
      "config": {
        "runtime": "nodejs18.x"
      }
    },
    {
      "src": "writing.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/generate"
    },
    {
      "src": "/",
      "dest": "/writing.html",
      "headers": {
        "Cache-Control": "public, max-age=3600"
      }
    }
  ]
}
